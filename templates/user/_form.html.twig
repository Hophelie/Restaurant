<div class="container">
    {{ form_start(form) }}
        <div class="col-6">
            {{ form_row(form.email,{'attr': {'class': 'form-control'}} ) }}
        </div>
        <div class="col-6">
            {{ form_row(form.nom,{'attr': {'class': 'form-control'}} ) }}
        </div>
        <div class="col-6">
            {{ form_row(form.prenom,{'attr': {'class': 'form-control'}} ) }}
        </div>
        <button  class="btn primary" class="btn">{{ button_label|default('Save') }}
        </button>
    {{ form_end(form) }}
</div>
